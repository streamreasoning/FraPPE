    <!-- http://purl.org/NET/c4dm/event.owl#agent -->

    <owl:ObjectProperty rdf:about="&c4dm;event.owl#agent">
        <rdfs:label>agent</rdfs:label>
        <rdfs:comment>
		Relates an event to an active agent (a person, a computer, ... :-) )
		</rdfs:comment>
        <ns:term_status>stable</ns:term_status>
        <rdfs:domain rdf:resource="&c4dm;event.owl#Event"/>
        <owl:inverseOf rdf:resource="&c4dm;event.owl#agent_in"/>
        <owl:equivalentProperty rdf:resource="&c4dm;event.owl#hasAgent"/>
        <rdfs:range rdf:resource="&foaf;Agent"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/NET/c4dm/event.owl#agent_in -->

    <owl:ObjectProperty rdf:about="&c4dm;event.owl#agent_in">
        <ns:term_status>stable</ns:term_status>
        <owl:equivalentProperty rdf:resource="&c4dm;event.owl#isAgentIn"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/NET/c4dm/event.owl#factor -->

    <owl:ObjectProperty rdf:about="&c4dm;event.owl#factor">
        <rdfs:label>factor</rdfs:label>
        <rdfs:comment>
		Relates an event to a passive factor (a tool, an instrument, an abstract cause...)
		</rdfs:comment>
        <ns:term_status>stable</ns:term_status>
        <rdfs:domain rdf:resource="&c4dm;event.owl#Event"/>
        <owl:inverseOf rdf:resource="&c4dm;event.owl#factor_of"/>
        <owl:equivalentProperty rdf:resource="&c4dm;event.owl#hasFactor"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/NET/c4dm/event.owl#factor_of -->

    <owl:ObjectProperty rdf:about="&c4dm;event.owl#factor_of">
        <ns:term_status>stable</ns:term_status>
        <owl:equivalentProperty rdf:resource="&c4dm;event.owl#isFactorOf"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/NET/c4dm/event.owl#hasAgent -->

    <owl:ObjectProperty rdf:about="&c4dm;event.owl#hasAgent">
        <ns:term_status>deprecated</ns:term_status>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/NET/c4dm/event.owl#hasFactor -->

    <owl:ObjectProperty rdf:about="&c4dm;event.owl#hasFactor">
        <ns:term_status>deprecated</ns:term_status>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/NET/c4dm/event.owl#hasProduct -->

    <owl:ObjectProperty rdf:about="&c4dm;event.owl#hasProduct">
        <ns:term_status>deprecated</ns:term_status>
        <owl:equivalentProperty rdf:resource="&c4dm;event.owl#product"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/NET/c4dm/event.owl#hasSubEvent -->

    <owl:ObjectProperty rdf:about="&c4dm;event.owl#hasSubEvent">
        <ns:term_status>deprecated</ns:term_status>
        <owl:equivalentProperty rdf:resource="&c4dm;event.owl#sub_event"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/NET/c4dm/event.owl#isAgentIn -->

    <owl:ObjectProperty rdf:about="&c4dm;event.owl#isAgentIn">
        <rdfs:label>agent in</rdfs:label>
        <ns:term_status>deprecated</ns:term_status>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/NET/c4dm/event.owl#isFactorOf -->

    <owl:ObjectProperty rdf:about="&c4dm;event.owl#isFactorOf">
        <rdfs:label>factor of</rdfs:label>
        <ns:term_status>deprecated</ns:term_status>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/NET/c4dm/event.owl#place -->

    <owl:ObjectProperty rdf:about="&c4dm;event.owl#place">
        <rdfs:label>place</rdfs:label>
        <ns:term_status>stable</ns:term_status>
        <rdfs:comment>
		Relates an event to a spatial object.
		</rdfs:comment>
        <rdfs:domain rdf:resource="&c4dm;event.owl#Event"/>
        <rdfs:range rdf:resource="&wgs84_pos;SpatialThing"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/NET/c4dm/event.owl#producedIn -->

    <owl:ObjectProperty rdf:about="&c4dm;event.owl#producedIn">
        <rdfs:label>produced in</rdfs:label>
        <ns:term_status>deprecated</ns:term_status>
        <owl:equivalentProperty rdf:resource="&c4dm;event.owl#produced_in"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/NET/c4dm/event.owl#produced_in -->

    <owl:ObjectProperty rdf:about="&c4dm;event.owl#produced_in">
        <ns:term_status>stable</ns:term_status>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/NET/c4dm/event.owl#product -->

    <owl:ObjectProperty rdf:about="&c4dm;event.owl#product">
        <rdfs:label>product</rdfs:label>
        <rdfs:comment>
		Relates an event to something produced during the event---a sound, a pie, whatever...
		</rdfs:comment>
        <ns:term_status>stable</ns:term_status>
        <rdfs:domain rdf:resource="&c4dm;event.owl#Event"/>
        <owl:inverseOf rdf:resource="&c4dm;event.owl#produced_in"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/NET/c4dm/event.owl#sub_event -->

    <owl:ObjectProperty rdf:about="&c4dm;event.owl#sub_event">
        <rdfs:label>sub-event</rdfs:label>
        <rdfs:comment>
		This property provides a way to split a complex event (for example, a performance involving several 
		musicians) into simpler ones (one event per musician).
		</rdfs:comment>
        <ns:term_status>stable</ns:term_status>
        <rdfs:domain rdf:resource="&c4dm;event.owl#Event"/>
        <rdfs:range rdf:resource="&c4dm;event.owl#Event"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/NET/c4dm/event.owl#time -->

    <owl:ObjectProperty rdf:about="&c4dm;event.owl#time">
        <rdfs:label>time</rdfs:label>
        <ns:term_status>stable</ns:term_status>
        <rdfs:comment>
		Relates an event to a time object, classifying a time region (either instantaneous or having an extent).
		By using the Timeline ontology here, you can define event happening on a recorded track or on any 
		media with a temporal extent.
		</rdfs:comment>
        <rdfs:domain rdf:resource="&c4dm;event.owl#Event"/>
        <rdfs:range rdf:resource="&time;TemporalEntity"/>
    </owl:ObjectProperty>
    


    <!-- http://xmlns.com/foaf/0.1/maker -->

    <owl:ObjectProperty rdf:about="&foaf;maker"/>
    


