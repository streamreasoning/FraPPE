     -->

    


    <!-- http://purl.org/NET/c4dm/event.owl#hasLiteralFactor -->

    <owl:DatatypeProperty rdf:about="&c4dm;event.owl#hasLiteralFactor">
        <ns:term_status>deprecated</ns:term_status>
        <owl:equivalentProperty rdf:resource="&c4dm;event.owl#literal_factor"/>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/NET/c4dm/event.owl#literal_factor -->

    <owl:DatatypeProperty rdf:about="&c4dm;event.owl#literal_factor">
        <rdfs:label>literal factor</rdfs:label>
        <ns:term_status>stable</ns:term_status>
        <rdfs:comment>
		Relates an event to a factor which can be described as a literal. This property
		should not be used as-is, but should be subsumed by other, more specific, properties
		(like an hypothetic :weatherCelsius, linking an event to a temperature).
		</rdfs:comment>
        <rdfs:domain rdf:resource="&c4dm;event.owl#Event"/>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/dc/elements/1.1/date -->

    <owl:DatatypeProperty rdf:about="&dc;date"/>
    


    <!-- http://purl.org/dc/elements/1.1/description -->

    <owl:DatatypeProperty rdf:about="&dc;description"/>
    


    <!-- http://purl.org/dc/elements/1.1/title -->

    <owl:DatatypeProperty rdf:about="&dc;title"/>
    


