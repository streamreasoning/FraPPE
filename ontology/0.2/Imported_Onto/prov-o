        <rdfs:label>Person</rdfs:label>
        <rdfs:subClassOf rdf:resource="&ns;prov#Agent"/>
        <prov:dm rdf:datatype="&xsd;anyURI">http://www.w3.org/TR/2013/REC-prov-dm-20130430/#term-agent</prov:dm>
        <prov:n rdf:datatype="&xsd;anyURI">http://www.w3.org/TR/2013/REC-prov-n-20130430/#expression-types</prov:n>
        <prov:category>expanded</prov:category>
