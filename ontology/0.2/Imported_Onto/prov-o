    <owl:Class rdf:about="&ns;prov#Start">
        <rdfs:label>Start</rdfs:label>
        <rdfs:subClassOf rdf:resource="&ns;prov#EntityInfluence"/>
        <rdfs:subClassOf rdf:resource="&ns;prov#InstantaneousEvent"/>
        <prov:constraints rdf:datatype="&xsd;anyURI">http://www.w3.org/TR/2013/REC-prov-constraints-20130430/#prov-dm-constraints-fig</prov:constraints>
