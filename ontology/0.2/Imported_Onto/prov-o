    <owl:ObjectProperty rdf:about="&ns;prov#specializationOf">
        <rdfs:label>specializationOf</rdfs:label>
        <prov:constraints rdf:datatype="&xsd;anyURI">http://www.w3.org/TR/2013/REC-prov-constraints-20130430/#prov-dm-constraints-fig</prov:constraints>
